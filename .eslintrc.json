{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true
  },
  "plugins": ["svelte3", "prettier"],
  "extends": ["plugin:prettier/recommended"],
  "overrides": [
    {
      "files": ["*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "endOfLine":"auto"
    }]
  }
}